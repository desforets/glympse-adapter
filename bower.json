{
  "name": "glympse-adapter",
  "description": "Web-based adapter for handling Glympse invites, Glympse cards, creating/extending Glympse Viewer and custom APIs locally and across iframe boundaries, and instantiating the Glympse Viewer. Operates in host (connects to a client-mode adapter) or client (local interfaces or for host-mode adapters) modes.",
  "version": "1.3.6",
  "private": false,
  "homepage": "https://github.com/Glympse/glympse-adapter",
  "authors": [
    {
      "name": "Glympse Inc.",
      "email": "jason@glympse.com",
      "homepage": "http://github.com/Glympse/"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/Glympse/glympse-adapter.git"
  },
  "main": "app/src/",
  "moduleType": [
    "amd"
  ],
  "keywords": [
    "Glympse",
    "iframe",
    "cards",
    "viewer",
    "adapter"
  ],
  "license": "MPLv2",
  "ignore": [
    "**/.*",
    "*.html",
    "node_modules",
    "app/css",
    "app/lib",
	"app/src/requireConfig*",
	"app/src/test*",
    "test",
    "tests",
    "frag.*",
    "grunt",
    "Gruntfile.js",
	"package.json"
  ],
  "dependencies": {
    "requirejs": "~2.1.11",
    "almond": "~0.3.0",
    "MessageChannel.js": "~0.1.5",
    "UUID.js": "https://github.com/LiosK/UUID.js.git#2fe182e3166f9a279738568a75177011f26aa008",
    "kamino.js": "~0.0.2"
  },
  "devDependencies": {}
}
